@model IEnumerable<DpApiClient.Data.Specialization>

@{
    ViewBag.Title = "Specializations";
}

<h2>Specializations</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>
